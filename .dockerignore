*.pyc
*.pyo
*.pyd
*.pyc
*.pyo
*.pyd
.Python
env/
venv/
.venv/
ENV/
env.bak/
venv.bak/

# Ignore Jupyter Notebook checkpoints
.ipynb_checkpoints

# Ignore local data, logs, and outputs
data/
logs/
*.log

# Ignore OS files
.DS_Store
Thumbs.db

# Ignore git and version control
.git/
.gitignore

# Ignore editor/project files
.vscode/
.idea/
